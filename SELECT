SELECT NOPEMAIN, NAMA, TANGGAL_LAHIR
FROM PEMAIN
WHERE KOTA = 'Siantar'
ORDER BY NAMA;

SELECT NOPEMAIN
FROM PEMAIN
WHERE BERGABUNG > 2000
ORDER BY NOPEMAIN;

